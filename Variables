// Program using variables

#include <stdio.h>

int main ()
{
    int a = 5;
    float b = 2.5;
    char c = 'j';
    int d = 10;

    printf ("The value of a is %d \n" , a);
    printf ("The value of b is %f \n" , b);
    printf ("The character is %c \n" , c);
    printf ("The sum of a & d is %d \n" , a+d);

    return 0;
}
